{
    "hosting": {
        "public": "./dist/go-escuela-pnpm",
        "rewrites": [{
            "source": "**",
            "destination": "/index.html"
        }],
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "predeploy": [
            "pnpm run build"
        ]
    }
}